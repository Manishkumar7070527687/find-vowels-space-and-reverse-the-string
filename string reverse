#include<iostream>
using namespace std;
#include<string.h>
                                              //vowel space and reverse kaise krenege y code kiye h.
int main(){

    char str[100]="Geetanm university";

    int space=0;
    int vowel=0;

    for(int i=0 ; i<strlen(str) ; i++){
        if((str[i]=='a')||(str[i]=='e')||(str[i]=='i')||(str[i]=='o')||(str[i]=='u')){
            vowel++;
        }
        
        if(str[i]==' '){
            space++;
        }
    }
    cout<<space<<" "<<vowel;
    int length=strlen(str);

    for(int i=length-1;i>=0;i--){
        cout<<str[i];
    }


    return 0;
}
